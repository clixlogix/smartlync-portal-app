    "scripts": {
        "start": "react-scripts start",
        "build": "react-app-rewired build",
        "dev:env": "REACT_APP_BASE_URL=https://khxrtmoxri.execute-api.eu-central-1.amazonaws.com/dev REACT_APP_ENVIRONMENT=development",
        "dev:env:local": "env-cmd -f .env.development",
        "start:dev": "npm run dev:env react-scripts start",
        "build:dev": "npm run dev:env react-scripts build",
        "stage:env": "REACT_APP_BASE_URL=https://r8miqbjwmc.execute-api.eu-central-1.amazonaws.com/staging REACT_APP_ENVIRONMENT=stage DEMO_REACT_APP_BASE_URL=https://khxrtmoxri.execute-api.eu-central-1.amazonaws.com/dev",
        "stage:env:local": "env-cmd -f .env.stage",
        "start:stage": "npm run stage:env react-scripts start",
        "build:stage": "npm run stage:env react-scripts build",
        "prod:env": "REACT_APP_BASE_URL=https://lpfcak0080.execute-api.eu-central-1.amazonaws.com/prod REACT_APP_ENVIRONMENT=production",
        "prod:env:local": "env-cmd -f .env.production",
        "start:prod": "npm run prod:env react-scripts start",
        "build:prod": "npm run prod:env react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/test-generators.ts",
        "checkTs": "tsc --noEmit",
        "eslint": "eslint --ext js,ts,tsx,json",
        "lint": "yarn run eslint src",
        "lint:fix": "npm run eslint --fix src",
        "lint:css": "stylelint src/**/*.css",
        "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
        "cleanAndSetup": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/clean.ts",
        "prettify": "prettier --write",
        "extract-messages": "i18next-scanner --config=internals/extractMessages/i18next-scanner.config.js",
        "prepare": "husky install"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "lint-staged": {
        "*.{ts,tsx,js,jsx}": [
            "yarn run eslint --fix"
        ],
        "*.{md,json}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run checkTs && lint-staged && npm run verify-startingTemplate-changes",
            "prepare-commit-msg": "devmoji -e",
            "commit-msg": "if git-branch-is dev; then commitlint -E HUSKY_GIT_PARAMS; fi"
        }
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*/*.d.ts",
            "!src/**/*/Loadable.{js,jsx,ts,tsx}",
            "!src/**/*/messages.ts",
            "!src/reportWebVitals.ts",
            "!src/**/*/types.ts",
            "!src/index.tsx",
            "!src/serviceWorker.ts"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 90,
                "functions": 90,
                "lines": 90,
                "statements": 90
            }
        }
    },
